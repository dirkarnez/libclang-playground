    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach function-visitor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\cmake-build\\function-visitor\\function-visitor.exe",
            "cwd": "${workspaceFolder}\\cmake-build\\function-visitor",
            "args": [
                "${workspaceFolder}\\lib\\LLVM-21.1.2-win64\\include\\clang-c\\BuildSystem.h"
            ],
            "MIMode": "gdb",
            "environment": [],
            "externalConsole": false,
            "miDebuggerPath": "${env:USERPROFILE}\\Downloads\\winlibs-x86_64-posix-seh-gcc-11.2.0-mingw-w64-9.0.0-r1\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Attach ast-walker",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\cmake-build\\ast-walker\\ast-walker.exe",
            "cwd": "${workspaceFolder}\\cmake-build\\ast-walker",
            "args": [
                "${workspaceFolder}\\lib\\LLVM-21.1.2-win64\\include\\clang-c\\BuildSystem.h"
            ],
            "MIMode": "gdb",
            "environment": [],
            "externalConsole": false,
            "miDebuggerPath": "${env:USERPROFILE}\\Downloads\\winlibs-x86_64-posix-seh-gcc-11.2.0-mingw-w64-9.0.0-r1\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}